<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="CloudLogger" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2014-06.2.4">
 <WebRole name="WebService" vmsize="Small">
  <Sites>
   <Site name="Web">
    <Bindings>
     <Binding name="Endpoint1" endpointName="Endpoint1" />
    </Bindings>
   </Site>
  </Sites>
  <Endpoints>
   <InputEndpoint name="Endpoint1" protocol="http" port="80" />
  </Endpoints>
  <Startup>
   <Task commandLine="setup_web.cmd &gt; log.txt" executionContext="elevated" taskType="simple">
    <Environment>
     <Variable name="EMULATED">
      <RoleInstanceValue xpath="/RoleEnvironment/Deployment/@emulated" />
     </Variable>
     <Variable name="RUNTIMEID" value="node" />
     <Variable name="RUNTIMEURL" value="http://az413943.vo.msecnd.net/node/0.10.21.exe;http://nodertncu.blob.core.windows.net/iisnode/0.1.21.exe" />
    </Environment>
   </Task>
  </Startup>
 </WebRole>
</ServiceDefinition>